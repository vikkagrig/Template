<Window x:Class="Template_4337.Grigoreva"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Template_4337"
        mc:Ignorable="d"
        Title="Grigoreva" Height="450" Width="800">
    <Grid>
        <StackPanel Margin="20" Orientation="Vertical" HorizontalAlignment="Center" VerticalAlignment="Center">
            <Button x:Name="btn1" Content="Импорт данных" FontSize="30" Margin="0 10" Click="btn1_Click"/>
            <Button x:Name="btn2" Content="Экспорт в EXCEL" FontSize="30" Margin="0 10" Click="btn2_Click"/>
            <Button x:Name="btn3" Content="Импорт JSON данных" FontSize="30" Margin="0 10" Click="btn3_Click"/>
            <Button x:Name="btn4" Content="Экспорт в WORD" FontSize="30" Margin="0 10" Click="btn4_Click"/>
        </StackPanel>
    </Grid>
</Window>
